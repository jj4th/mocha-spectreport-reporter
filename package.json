{
  "name": "mocha-spectreport-reporter",
  "version": "1.4.0",
  "description": "A mocha reporter for use with Spectreport",
  "main": "./lib/spectreport-reporter.min.js",
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "coverage": "gulp coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jj4th/mocha-spectreport-reporter.git"
  },
  "keywords": [
    "mocha",
    "spectreport",
    "test"
  ],
  "author": "John J. Andrichak IV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jj4th/mocha-spectreport-reporter/issues"
  },
  "homepage": "https://github.com/jj4th/mocha-spectreport-reporter",
  "devDependencies": {
    "babel": "^5.8.35",
    "babel-core": "^5.8.35",
    "babel-eslint": "^2.0.2",
    "babel-runtime": "^6.3.19",
    "chai": "^2.1.2",
    "coveralls": "^2.11.6",
    "del": "^1.1.1",
    "eslint": "^0.17.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.2.1",
    "gulp-concat": "^2.5.2",
    "gulp-eslint": "^0.7.0",
    "gulp-file": "^0.2.0",
    "gulp-filter": "^2.0.2",
    "gulp-istanbul": "^0.6.0",
    "gulp-istanbul-enforcer": "^1.0.3",
    "gulp-load-plugins": "^0.8.1",
    "gulp-mocha": "^2.0.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^1.2.0",
    "isparta": "^2.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.1",
    "proxyquire": "^1.4.0",
    "rollup": "^0.15.0",
    "sinon": "^1.14.1",
    "sinon-as-promised": "^3.0.0",
    "sinon-chai": "^2.7.0",
    "webpack": "^1.12.11"
  },
  "taskConfig": {
    "exportVarName": "spectreportReporter",
    "entryFileName": "src/index",
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "threshold": {
    "statements": 100,
    "branches": 100,
    "lines": 100,
    "functions": 100
  },
  "config": {
    "loglevel": "silent"
  },
  "dependencies": {
    "mocha": "^2.2.4",
    "fs-extra": "^0.18.2",
    "spectreport": "^1.0.0"
  }
}
